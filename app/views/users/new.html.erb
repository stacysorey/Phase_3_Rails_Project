<h1> sign up </h1>

<%= form_with(model: @user, url: signup_path)  do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %><br> 
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <%= f.submit "sign up" %>
<% end %>