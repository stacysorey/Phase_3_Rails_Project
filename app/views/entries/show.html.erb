<h1> <%= @entry.title%></h1>
<h3> <%= format_date(@entry) %></h3>
<h3><i><%= @entry.entry_type.category%></i></h3><br>
<p> <%= @entry.description%></p>

<div class= "bctr">
  <%= link_to "edit your entry", edit_journal_entry_path(@journal.id, @entry.id), class: "b4" %>
  <%= link_to 'back', journal_entries_path(@journal.id), class: "b4" %>

</div>

<div class="btn-holder">
<%= link_to "delete entry", journal_entry_path(@journal.id, @entry.id), method: "delete", class: "b7" %>
</div>


